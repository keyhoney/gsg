{
  "name": "gsg",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:pages\"",
    "dev:client": "vite build --watch --mode development",
    "dev:pages": "wrangler pages dev dist --local --persist-to .wrangler/state",
    "build": "npm run clean && npm run build:client",
    "build:client": "vite build",
    "clean": "rimraf dist .wrangler .cloudflare"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.1",
    "@lucia-auth/adapter-sqlite": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-query": "^5.55.4",
    "@types/katex": "^0.16.7",
    "@types/qrcode": "^1.5.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "hono": "^4.5.3",
    "katex": "^0.16.22",
    "ky": "^1.6.0",
    "lucia": "^3.1.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.27.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "rimraf": "^5.0.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240909.0",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.4",
    "vite": "^5.4.7",
    "wrangler": "^4.40.3"
  }
